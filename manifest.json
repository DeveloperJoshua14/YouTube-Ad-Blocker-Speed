{
    "manifest_version": 3,
    "name": "YouTube Ad Speed Up",
    "version": "2.0.1",
    "description": "Speeds up YouTube ads by increasing the playback rate.",
    "permissions": [
      "scripting",
      "activeTab"
    ],
    "host_permissions": [
      "*://www.youtube.com/*"
    ],
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  